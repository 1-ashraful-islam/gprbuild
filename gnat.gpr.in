with "shared";
project GNAT is
   for Source_Dirs use ("@GNAT_SOURCE_DIR@");
   for Object_Dir  use Shared'Object_Dir;
   for Source_List_File use "@GNAT_SOURCE_DIR@/MANIFEST.GPRMAKE";


   package Compiler is
      for Default_Switches ("Ada") use Shared.Compiler'Default_Switches("Ada") & ("-gnatpg");
   end Compiler;

   package Naming is
      for Body ("sdefault") use "gprmake_sdefault.adb";
   end Naming;

end GNAT;
