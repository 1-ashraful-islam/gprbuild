standard project Prj is
   type BUILD_KIND is ("default");
   BUILD : BUILD_KIND := external("PRJ_BUILD", "default");

   for Languages use ("Ada");

   case BUILD is
      when "default" =>
         for Source_Dirs use ("../../include/prj");
         for Object_Dir use "../../lib/prj";
   end case;


   package Naming is
      for dot_replacement use "-";
      for casing use "lowercase";

      case BUILD is
         when "default" =>
            for body_suffix ("ada") use ".adb";
            for spec_suffix ("ada") use ".ads";
      end case;
   end Naming;

   package Linker is
      case BUILD is
         when "default" =>
            null;
      end case;
   end Linker;

   package Install is
      for Active use "False";
   end Install;

   for Externally_Built use "True";
end Prj;

