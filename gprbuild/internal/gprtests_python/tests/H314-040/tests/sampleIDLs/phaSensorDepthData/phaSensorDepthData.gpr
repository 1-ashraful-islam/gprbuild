with "../../../dds.gpr";
with "../../../gnat_default.gpr";
with "../RootPackages/rootpackages.gpr";
with "../timeType/timeType.gpr";
Project PhaSensorDepthData is

   for Languages use ("Idl", "Ada", "C");
   for Source_Dirs use (".", "generated");
   for Object_Dir use Gnat_Default'Object_Dir;
   Name := "phaSensorDepthData";
   case Gnat_Default.Library_Kind is
      when "None" =>
      when "static" | "dynamic" =>
         for Library_Kind    use Gnat_Default'Library_Kind;
         for Library_Dir     use Gnat_Default'Library_Dir;
         for Library_Src_Dir use Gnat_Default'Library_Src_Dir;
         for Library_Name use Name;
         case Gnat_Default.Library_Kind is
            when "dynamic" =>
               for Library_Auto_Init use "True";
               for Library_Name use Project'Library_Name & Gnat_Default.Dynamic_Lib_Sufix;
               for Library_Interface use ("com.saabgroup.cms.pha.sensor.depth",
                                          "com.saabgroup.cms.pha.sensor.depth.depth",
                                          "com.saabgroup.cms.pha.sensor.depth.depth_datareader",
                                          "com.saabgroup.cms.pha.sensor.depth.depth_datawriter",
                                          "com.saabgroup.cms.pha.sensor.depth.depth_seq",
                                          "com.saabgroup.cms.pha.sensor.depth.depth_typesupport",
                                          "com.saabgroup.cms.pha.sensor.depth.depthtype",
                                          "com.saabgroup.cms.pha.sensor.depth.depthtype_seq");
            when "static" =>
               for Library_Name use Project'Library_Name & Gnat_Default.Static_Lib_Sufix;
            when "None" =>
               for Library_Name use Project'Library_Name & Gnat_Default.Static_Lib_Sufix;
         end case;

         case Gnat_Default.Build is
            when "release" =>
               for Library_Name use Project'Library_Name & Gnat_Default.Release_Lib_Sufix;
            when "debug" =>
               for Library_Name use Project'Library_Name & Gnat_Default.Debug_Lib_Sufix;
            when "normal" =>
               for Library_Name use Project'Library_Name & Gnat_Default.Normal_Lib_Sufix;
         end case;
   end case;


   package Binder is
      for Default_Switches ("ada") use
        Gnat_Default.Binder'Default_Switches ("ada");
   end Binder;

   package Compiler is
      for Driver ("idl") use "";
      for Default_Switches ("ada") use
        DDS.Compiler'Default_Switches ("ada") &
      Gnat_Default.Compiler'Default_Switches ("ada");
      for Default_Switches ("C") use
        DDS.Compiler'Default_Switches ("C") &
      Gnat_Default.Compiler'Default_Switches ("C");
      for Default_Switches ("idl") use ("-language", "Ada",
                                        "-replace",
                                        "-xml",
                                        "-example", "i86Win32gcc4.1.3",
                                        "-d", "generated");
   end Compiler;

   package Builder is
      for Default_Switches ("ada") use
        Gnat_Default.Builder'Default_Switches ("ada");
   end Builder;

   package Naming is
      for Implementation_Suffix ("idl") use ".idl";
   end Naming;

   package Linker is
      for Default_Switches ("ada") use
        DDS.Linker'Default_Switches ("ada");
   end Linker;

   package Gnatstub renames Gnat_Default.Gnatstub;
   package Pretty_Printer renames Gnat_Default.Pretty_Printer;


   package IDE is
      for Vcs_Kind use "Subversion";
   end IDE;

end PhaSensorDepthData;

