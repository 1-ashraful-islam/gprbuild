aggregate library project Sa_Lib3 is
   for Project_Files use ("sa_lib.gpr", "sa_lib2.gpr");
   for Library_Dir  use "lib3";
   for Library_Kind use "dynamic";
   for Library_Interface use ("ada_lib2");
   for Library_Name use "sa3";
   for Library_Standalone use "encapsulated";
   for Library_Options use
     ("-L" & project'Project_Dir & "/extlib/lib", "-ltoto");
end Sa_Lib3;
