with "subsystem.gpr";
project C_Main is
   for Languages    use ("C");
   for Source_Dirs  use ("c_src");
   for Object_Dir   use  "c_obj";
   for Main         use ("c_main.c");
   for Roots ("c_main.c") use ("*");
   for Exec_Dir     use ".";
   package Compiler is
      for Default_Switches ("C")  use ("-DXXX");
      for Switches ("c_main.c")    use
        Compiler'Default_Switches ("C") & ("-DYYY");
   end Compiler;
end C_Main;
