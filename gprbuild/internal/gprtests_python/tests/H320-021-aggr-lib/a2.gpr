library project A2 is
   type Library_Kind is ("relocatable", "static");
   Library_Type : Library_Kind := external ("LIBRARY_TYPE", "static");

   for source_Dirs use ("a2_src");
   for library_name use "a2";
   for library_dir use "a2lib";
   for object_dir use "o2lib";
   for library_kind use Library_Type;

   package Compiler is
      for Default_Switches ("Ada") use ("-O2");
   end Compiler;
end A2;
