with "subsystem.gpr";
project C_Main is
   for Languages    use ("C");
   for Source_Dirs  use ("c_src");
   for Object_Dir   use  "c_obj";
   for Main         use ("c_main.c");
   for Roots ("c_main.c") use ("*");
   for Exec_Dir     use ".";

   package Linker is
      for Driver use "gcc";
   end Linker;
end C_Main;
