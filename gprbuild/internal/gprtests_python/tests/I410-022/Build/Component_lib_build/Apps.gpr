with "Common";
limited with "Component_B";
limited with "Component_C";

project Apps is

   for Source_Dirs use (external("GNAT_PROJ_ROOT") & "\Build\Source\Source1",
                        external("GNAT_PROJ_ROOT") & "\Build\Source\Source2");

   for Object_Dir use external("GNAT_PROJ_ROOT") & "\Build\Objects\Apps";

   for Library_Name use "Apps";
   
   for Library_Dir use external("GNAT_PROJ_ROOT") & "\Build\Library";

   for Library_Kind use "static";

   package Builder renames Common.Builder;

   package Compiler renames Common.Compiler;

   package Binder renames Common.Binder;

   package Linker renames Common.Linker;

   package Pretty_Printer renames Common.Pretty_Printer;

end Apps;

