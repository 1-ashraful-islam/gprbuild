
configuration project Default is
   for Target use "i686-pc-linux-gnu";

   package Compiler is
      from "GNAT" node
      PREFIX=
      VERSION=6.1
      RUNTIME_DIR=<pwd>/install/lib/gcc/i686-pc-linux-gnu/3.4.6/rts-sjlj/
      from "GNAT" and not "mingw" target
      from "GNAT" and "sjlj" runtime
   end Compiler;
end Default;
